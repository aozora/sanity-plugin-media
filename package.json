{
  "name": "sanity-plugin-media-v4",
  "version": "4.0.0",
  "description": "This version of `sanity-plugin-media` is for Sanity Studio v4.",
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "sanity-plugin",
    "asset",
    "browser"
  ],
  "license": "MIT",
  "author": "Marcello Palmitessa",
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "plugin-kit verify-package --silent && pkg-utils build --strict --check --clean",
    "clean": "rm -fr dist",
    "dev": "sanity dev",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "watch": "pkg-utils watch --strict"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.1.1",
    "@reduxjs/toolkit": "^2.6.0",
    "@sanity/client": "^7.11.1",
    "@sanity/color": "^3.0.6",
    "@sanity/icons": "^3.7.4",
    "@sanity/incompatible-plugin": "^1.0.5",
    "@sanity/ui": "^3.1.0",
    "@sanity/uuid": "^3.0.2",
    "@tanem/react-nprogress": "^5.0.55",
    "copy-to-clipboard": "^3.3.1",
    "date-fns": "^4.1.0",
    "filesize": "^10.1.6",
    "groq": "^4.8.1",
    "is-hotkey-esm": "^1.0.0",
    "nanoid": "5.0.9",
    "pluralize": "^8.0.0",
    "react-dropzone": "^14.3.8",
    "react-file-icon": "^1.6.0",
    "react-hook-form": "^7.62.0",
    "react-redux": "^9.2.0",
    "react-select": "^5.10.2",
    "react-virtuoso": "^4.14.0",
    "redux": "^5.0.1",
    "redux-observable": "3.0.0-rc.2",
    "rxjs": "^7.8.2",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@sanity/eslint-config-studio": "^5.0.2",
    "@sanity/pkg-utils": "^8.1.9",
    "@sanity/plugin-kit": "^4.0.19",
    "@sanity/semantic-release-preset": "^2.0.5",
    "@sanity/vision": "^4.8.1",
    "@types/is-hotkey": "^0.1.10",
    "@types/pluralize": "^0.0.33",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/react-file-icon": "^1.0.4",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-config-sanity": "^7.1.4",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-is": "^19.1.1",
    "sanity": "^4.8.1",
    "styled-components": "^6.1.19",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "react": "^18.3 || ^19",
    "react-dom": "^18.3 || ^19",
    "react-is": "^18.3 || ^19",
    "sanity": "^3.78 || ^5.0.0-0",
    "styled-components": "^6.1"
  },
  "engines": {
    "node": ">=18"
  }
}
